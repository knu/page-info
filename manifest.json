{
  "name": "Page Info",
  "description": "Show the page info and the canonical URL",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Page Info"
  },
  "background": {
    "service_worker": "src/worker.ts",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "webNavigation",
    "clipboardWrite",
    "storage"
  ],
  "host_permissions": ["<all_urls>"]
}
